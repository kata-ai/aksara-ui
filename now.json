{
  "version": 2,
  "name": "wicara-docs",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "buildCommand": "yarn run build:storybook", "distDir": "storybook-static" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/static/$1"
    },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "index.html"
    }
  ]
}
