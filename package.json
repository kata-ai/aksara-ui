{
  "name": "kata-kit",
  "version": "0.1.1",
  "description": "UI Kit for React from Kata.ai",
  "author": "kata-ai",
  "license": "MIT",
  "repository": "kata-ai/kata-kit",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 node scripts/test.js",
    "test:watch": "node scripts/test.js",
    "test:coverage": "cross-env CI=1 node scripts/test.js --coverage",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "report-coverage": "codecov -f ./coverage/*.json",
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "dom-helpers": "^3.3.1",
    "lodash-es": "^4.17.10",
    "moment": "^2.22.1",
    "react-addons-shallow-compare": "^15.6.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dates": "^12.7.1",
    "react-overlays": "^0.8.3",
    "shortid": "^2.2.8"
  },
  "peerDependencies": {
    "react": "16.3.0",
    "react-dom": "16.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-overlays": "^0.8.4",
    "@types/shortid": "0.0.29",
    "@types/sinon": "^4.3.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^4.1.1",
    "codecov": "^3.0.2",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.4",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "jest-enzyme": "^6.0.0",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "minicat": "^1.0.0",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-scripts": "^1.1.1",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.1.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-svg": "^1.0.1",
    "rollup-plugin-typescript2": "^0.14.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-url": "^1.3.0",
    "sinon": "^5.0.7",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-kata": "^1.0.4",
    "typescript": "^2.8.3"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{tsx}",
      "!src/components/index.tsx"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
