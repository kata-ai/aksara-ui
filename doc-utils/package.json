{
  "name": "@kata-kit/doc-utils",
  "version": "0.5.2",
  "description": "Utilities to help with building the kata-kit documentation.",
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "kata-kit:src": "./src/index.ts",
  "repository": "https://github.com/kata-ai/kata-kit.git",
  "author": "Kata.ai <info@kata.ai>",
  "license": "Apache-2.0",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn clean && yarn tsc && yarn rollup",
    "clean": "rimraf lib && rimraf tsc-out",
    "rollup": "rollup -i ./tsc-out/index.js -c ../rollup.config.js",
    "tsc": "tsc -b",
    "watch": "yarn rollup -w",
    "watch:tsc": "tsc-watch -b --preserveWatchOutput"
  },
  "dependencies": {
    "@kata-kit/badge": "^0.5.2",
    "@kata-kit/button": "^0.5.2",
    "@kata-kit/loading": "^0.5.2",
    "@kata-kit/theme": "^0.5.2",
    "polished": "^2.1.1",
    "react-live": "^1.11.0",
    "styled-components": "^3.4.9"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6"
  }
}
