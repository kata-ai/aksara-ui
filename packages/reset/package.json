{
  "name": "@kata-kit/reset",
  "version": "0.6.0",
  "description": "Provides necessary CSS resets to use kata-kit.",
  "main": "./lib/index.css",
  "kata-kit:src": "./src/index.scss",
  "repository": "https://github.com/kata-ai/kata-kit.git",
  "author": "Kata.ai <info@kata.ai>",
  "license": "Apache-2.0",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf lib && yarn sass",
