    "@kata-kit/dropdown": "^0.5.0",
    "@kata-kit/fonts": "^0.5.0",
    "@kata-kit/form": "^0.5.0",
    "@kata-kit/layout": "^0.5.0",
    "@kata-kit/loading": "^0.5.0",
    "@kata-kit/modal": "^0.5.0",
