{
  "name": "@wicara/core",
  "version": "0.0.0",
  "description": "Core components for Wicara.",
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "wicara:src": "./src/index.ts",
  "repository": "https://github.com/kata-ai/wicara.git",
  "author": "Kata.ai <dev@kata.ai>",
  "license": "Apache-2.0",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn rollup",
    "clean": "rimraf lib && rimraf tsc-out",
    "prepublishOnly": "yarn build",
    "rollup": "rollup -i ./src/index.ts -c ../../rollup.config.js",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook",
    "tsc": "tsc -b .",
    "watch": "yarn rollup -w",
    "watch:tsc": "tsc-watch -b --preserveWatchOutput"
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "focus-trap": "^5.1.0",
    "polished": "^3.4.2",
    "react-transition-group": "^4.3.0",
    "styled-system": "^5.1.2"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "@storybook/addon-a11y": "5.2.1",
    "@storybook/addon-actions": "5.2.1",
    "@storybook/addon-knobs": "5.2.1",
    "@storybook/addon-links": "5.2.1",
    "@storybook/addon-notes": "5.2.1",
    "@storybook/react": "5.2.1",
    "@storybook/theming": "5.2.1",
    "@types/react-transition-group": "^4.2.3",
    "@types/styled-system": "^5.1.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-react-app": "^9.0.2"
  }
}
