{
  "name": "@wicara/core",
  "version": "0.7.0",
  "description": "Core components for Wicara.",
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "wicara:src": "./src/index.ts",
  "repository": "https://github.com/kata-ai/wicara.git",
  "author": "Kata.ai <dev@kata.ai>",
  "license": "Apache-2.0",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn rollup",
    "clean": "rimraf lib && rimraf tsc-out",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "yarn build",
    "report-coverage": "codecov -f ./coverage/*.json",
    "rollup": "rollup -i ./src/index.ts -c ../../rollup.config.js",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "watch": "yarn rollup -w",
    "watch:tsc": "tsc-watch -b --preserveWatchOutput"
  },
  "dependencies": {
    "@tippy.js/react": "^3.1.1",
    "clsx": "^1.0.4",
    "focus-trap": "^5.1.0",
    "polished": "^3.4.2",
    "react-transition-group": "^4.3.0",
    "styled-system": "^5.1.2"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "@storybook/addon-a11y": "5.2.1",
