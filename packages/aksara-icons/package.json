{
  "name": "@aksara/icons",
  "version": "0.8.0",
  "description": "Icon library for the Aksara design system.",
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "wicara:src": "./src/index.ts",
  "repository": "https://github.com/kata-ai/wicara.git",
  "author": "Kata.ai <dev@kata.ai>",
  "license": "Apache-2.0",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn storybook:build",
    "clean": "rimraf lib && rimraf tsc-out",
    "compile": "yarn clean && yarn rollup",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "yarn compile",
    "report-coverage": "codecov -f ./coverage/*.json",
    "rollup": "rollup -c ./rollup.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "validate": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "clsx": "^1.0.4"
  },
  "peerDependencies": {
    "react": "^16.10.0",
    "react-dom": "^16.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.7.4",
    "@babel/preset-typescript": "7.7.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "codecov": "^3.6.1",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "storybook-addon-jsx": "^7.1.13",
    "styled-components": "^4.4.1",
    "typescript": "~3.7.3"
  }
}
