{
  "name": "@aksara-ui-kit/icons",
  "version": "0.8.0",
  "description": "Icon library for the Aksara design system.",
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "wicara:src": "./src/index.ts",
  "repository": "https://github.com/kata-ai/wicara.git",
  "author": "Kata.ai <dev@kata.ai>",
  "license": "Apache-2.0",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn storybook:build",
    "clean": "rimraf lib && rimraf tsc-out",
    "compile": "yarn clean && yarn rollup",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "yarn compile",
    "report-coverage": "codecov -f ./coverage/*.json",
    "rollup": "rollup -c ./rollup.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "validate": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "clsx": "^1.0.4"
  },
  "peerDependencies": {
    "react": "^16.10.0",
    "react-dom": "^16.10.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "codecov": "^3.6.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "storybook-addon-jsx": "^7.1.13",
    "styled-components": "^4.4.1",
    "typescript": "~3.7.3"
  }
